{
  "name": "gocyclo_vscode_client",
  "displayName": "Gocyclo For Vscode",
  "description": "A VSCode extension for gocyclo on LSP",
  "version": "0.0.1",
  "publisher": "takaaa220",
  "engines": {
    "vscode": "^1.69.1"
  },
  "main": "./out/extension.js",
  "activationEvents": [
    "onLanguage:go"
  ],
  "contributes": {
    "languages": [
      {
        "id": "go",
        "aliases": [
          "Go",
          "go"
        ],
        "extensions": [
          ".go"
        ]
      }
    ],
    "commands": [
      {
        "command": "extension.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ."
  },
  "author": "takaaa220",
  "license": "MIT",
  "dependencies": {
    "vscode-languageclient": "^9.0.1"
  },
  "devDependencies": {
    "@types/vscode": "^1.94.0",
    "typescript": "^5.6.3"
  }
}
